{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "A simple Minecraft to Discord Chat bot with Whitelisting",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "homepage": "https://github.com/firstdarkdev/sdlink",
    "issues": "https://github.com/firstdarkdev/sdlink/issues",
    "discord": "https://discord.firstdark.dev"
  },
  "license": "MIT",
  "environment": "*",
  "entrypoints": {
    "server": [
      "com.hypherionmc.sdlink.SDLinkFabric"
    ],
    "client": [
      "com.hypherionmc.sdlink.client.SDLinkFabricClient"
    ]
  },
  "mixins": [
    "sdlink.mixins.json",
    "sdlink-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14",
    "fabric": "*",
    "minecraft": ">=1.20.2",
    "craterlib": "*",
    "java": ">=17"
  }
}